<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <a href="/new">Create new message</a>
    <% messages.forEach((message) => { %>
        <div>
            <p><%= message.text %></p>
            <p>By: <%= message.user %></p>
            <p>On: <%= message.added %></p>
        </div>
   <% }) %>
</body>
</html>